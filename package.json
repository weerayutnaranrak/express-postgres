{
  "name": "express-postgres",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "start": "nodemon src/index.ts",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "yarn typeorm migration:generate -d src/database/data-source.ts",
    "migration:create": "yarn typeorm migration:create",
    "migration:run": "yarn typeorm migration:run -d src/database/data-source.ts",
    "migration:revert": "yarn typeorm migration:revert -d src/database/data-source.ts",
    "migration:show": "yarn typeorm migration:show -d src/database/data-source.ts"
  },
  "dependencies": {
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.23",
    "@types/pg": "^8.6.5",
    "concurrently": "^7.4.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20",
    "path": "^0.12.7",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typeorm": "0.3.10",
    "typescript": "*"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.7.1",
    "ts-node": "10.7.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "4.5.2"
  }
}
